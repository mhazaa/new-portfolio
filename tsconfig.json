{
	"compilerOptions": {
		"target": "esnext",
		"jsx": "react",
		"module": "commonjs",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
	},
	"include": [
		"./backend/*.ts",
		"./@types/custom.d.ts",
	],
}